package Java201.methods;

public class MethodsLab {
	public static void main(String[] args) {
		
	System.out.println(max(1, 24, 21));	
	
	}
	
	public static int max(int x, int y, int z) {
		int max = x;
		if(y > max) {
			max = y;
		}
		else if(z > max) {
			max = z;
		}
		return max;
	}
	public static double max(double x, double y, double z) {
		double max = x;
		if(y > max) {
			max = y;
		}
		else if(z > max) {
			max = z;
		}
		return max;
	}
}
